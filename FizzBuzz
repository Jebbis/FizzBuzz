public class Fizzbuzz {

    

    public static void main(String[] args) {

        int kuinkaMontaKertaa = 101;
        
        for (int i = 1; i <kuinkaMontaKertaa; i++){
            String tuloste = "";
            if (i % 3 == 0) { tuloste += "Fizz"; }
            if (i % 5 == 0) { tuloste += "Buzz"; }
            if (output == "") { tuloste = Integer.toString(i); }
            System.out.println(tuloste);
        }
    }
}

    
